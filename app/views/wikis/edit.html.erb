<h1>Edit Wiki</h1>
<div class="">
  <%= form_for @wiki do |f| %>
       <div>
         <%= f.label :title %>
         <%= f.text_field :title, placeholder: "Enter wiki title" %>
       </div>
       <div>
         <%= f.label :body %>
         <%= f.text_area :body, placeholder: "Enter wiki body" %>
       </div>
       <div>
         <%= f.submit "Save", class: 'btn btn-success' %>
       </div>
     <% end %>
</div>
